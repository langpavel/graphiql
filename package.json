{
  "name": "@langpavel/graphiql",
  "version": "0.13.1",
  "description": "An graphical interactive in-browser GraphQL IDE.",
  "contributors": [
    "Hyohyeon Jeong <asiandrummer@fb.com>",
    "Lee Byron <lee@leebyron.com> (http://leebyron.com/)"
  ],
  "homepage": "https://github.com/graphql/graphiql",
  "bugs": {
    "url": "https://github.com/graphql/graphiql/issues"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/graphql/graphiql.git"
  },
  "license": "SEE LICENSE IN LICENSE",
  "main": "dist/index.js",
  "files": [
    "dist",
    "graphiql.js",
    "graphiql.min.js",
    "graphiql.css",
    "README.md",
    "LICENSE"
  ],
  "browserify-shim": {
    "react": "global:React",
    "react-dom": "global:ReactDOM"
  },
  "options": {
    "mocha": "--full-trace --require resources/mocha-bootload src/**/*-test.js"
  },
  "scripts": {
    "build": "bash ./resources/build.sh",
    "check": "flow check",
    "dev": "babel-node test/server.js",
    "lint": "eslint src",
    "lint-check": "eslint --print-config .eslintrc | eslint-config-prettier-check",
    "precommit": "npm run testonly && lint-staged",
    "prepublish": ". ./resources/prepublish.sh",
    "preversion": ". ./resources/checkgit.sh && npm test",
    "test": "npm run lint && npm run check && npm run testonly && npm run build",
    "testonly": "babel-node ./node_modules/.bin/_mocha $npm_package_options_mocha"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "codemirror": "^5.44.0",
    "codemirror-graphql": "^0.8.3",
    "markdown-it": "^8.4.2"
  },
  "peerDependencies": {
    "graphql": "^0.6.0 || ^0.7.0 || ^0.8.0-b || ^0.9.0 || ^0.10.0 || ^0.11.0 || ^0.12.0 || ^0.13.0 || ^14.0.0",
    "prop-types": ">=15.5.0",
    "react": "^15.6.0 || ^16.0.0",
    "react-dom": "^15.6.0 || ^16.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.4",
    "@babel/plugin-transform-regenerator": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.4.9",
    "babel-eslint": "10.0.1",
    "babelify": "10.0.0",
    "browserify": "^16.2.3",
    "browserify-shim": "3.8.14",
    "chai": "4.2.0",
    "chai-subset": "1.6.0",
    "eslint": "5.15.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-flowtype": "^3.4.2",
    "eslint-plugin-react": "7.12.4",
    "express": "5.0.0-alpha.5",
    "express-graphql": "0.7.1",
    "flow-bin": "0.94.0",
    "graphql": "^14.1.1",
    "husky": "^1.3.1",
    "jsdom": "13.2.0",
    "lint-staged": "^8.1.5",
    "mocha": "6.0.2",
    "postcss-cli": "6.1.2",
    "prettier": "^1.16.4",
    "prop-types": "15.7.2",
    "react": "16.8.3",
    "react-dom": "16.8.3",
    "react-test-renderer": "16.8.3",
    "uglify-js": "^3.4.9",
    "uglifyify": "^5.0.1",
    "watchify": "3.11.1"
  }
}
